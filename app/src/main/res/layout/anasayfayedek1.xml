<?xml version="1.0" encoding="utf-8"?>

<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/anaDrawerLayout">

    <RelativeLayout
        android:id="@+id/activity_anasayfa"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorPrimary"
        tools:context="com.falbookv4.helloteam.falbook.Activities.AnasayfaActivity">


        <android.support.constraint.ConstraintLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="567dp"
                android:id="@+id/guideline"
                android:background="@drawable/dikcizgi"
                android:orientation="vertical"
                tools:layout_editor_absoluteY="0dp"
                tools:layout_editor_absoluteX="192dp"
                app:layout_constraintGuide_percent="0.5" />

            <android.support.constraint.Guideline
                android:layout_width="384dp"
                android:layout_height="0dp"
                android:id="@+id/guideline2"
                android:orientation="horizontal"
                tools:layout_editor_absoluteY="207dp"
                tools:layout_editor_absoluteX="0dp"
                app:layout_constraintGuide_percent="0.36507937" />

            <android.support.constraint.Guideline
                android:layout_width="384dp"
                android:layout_height="0dp"
                android:id="@+id/guideline3"
                android:orientation="horizontal"
                tools:layout_editor_absoluteY="358dp"
                tools:layout_editor_absoluteX="0dp"
                app:layout_constraintGuide_percent="0.6313933" />

            <ImageButton
                android:layout_width="56dp"
                android:layout_height="56dp"
                android:background="@drawable/falbaktir"
                android:id="@+id/falbaktirButon"
                app:layout_constraintRight_toLeftOf="@+id/guideline"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="16dp"
                app:layout_constraintTop_toTopOf="@+id/guideline7"
                app:layout_constraintBottom_toTopOf="@+id/guideline2"
                android:layout_marginBottom="8dp"
                app:layout_constraintVertical_bias="0.39" />

            <TextView
                android:text="FAL BAKTIR"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/falbaktirText"
                android:fontFamily="sans-serif"
                android:textStyle="normal|bold"
                app:layout_constraintRight_toRightOf="@+id/falbaktirButon"
                app:layout_constraintLeft_toLeftOf="@+id/falbaktirButon"
                app:layout_constraintBottom_toTopOf="@+id/guideline2"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/falbaktirButon"
                app:layout_constraintVertical_bias="0.0" />

            <android.support.constraint.Guideline
                android:layout_width="384dp"
                android:layout_height="wrap_content"
                android:id="@+id/guideline7"
                android:orientation="horizontal"
                tools:layout_editor_absoluteY="56dp"
                tools:layout_editor_absoluteX="0dp"
                app:layout_constraintGuide_percent="0.09876543" />

            <android.support.constraint.Guideline
                android:layout_width="384dp"
                android:layout_height="wrap_content"
                android:id="@+id/guideline8"
                android:orientation="horizontal"
                tools:layout_editor_absoluteY="510dp"
                tools:layout_editor_absoluteX="0dp"
                app:layout_constraintGuide_percent="0.8994709" />

            <ImageButton
                android:layout_width="56dp"
                android:layout_height="56dp"
                android:background="@drawable/iletisimbuton"
                android:id="@+id/iletisimButon"
                app:layout_constraintTop_toTopOf="@+id/guideline3"
                app:layout_constraintBottom_toTopOf="@+id/guideline8"
                android:layout_marginBottom="8dp"
                app:layout_constraintLeft_toLeftOf="@+id/falbaktirButon"
                app:layout_constraintRight_toRightOf="@+id/falbaktirButon"
                app:layout_constraintVertical_bias="0.39" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/iletisimText"
                app:layout_constraintBottom_toTopOf="@+id/guideline8"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/iletisimButon"
                app:layout_constraintVertical_bias="0.0"
                android:text="İLETİŞİM"
                app:layout_constraintLeft_toLeftOf="@+id/iletisimButon"
                app:layout_constraintRight_toRightOf="@+id/iletisimButon"
                android:textStyle="normal|bold" />

            <ImageButton
                android:layout_width="56dp"
                android:layout_height="56dp"
                android:background="@drawable/telvekazanbuton"
                android:id="@+id/telvekazanButon"
                app:layout_constraintTop_toTopOf="@+id/guideline2"
                app:layout_constraintLeft_toLeftOf="@+id/falbaktirButon"
                app:layout_constraintRight_toRightOf="@+id/falbaktirButon"
                app:layout_constraintBottom_toTopOf="@+id/guideline3"
                android:layout_marginBottom="8dp"
                app:layout_constraintVertical_bias="0.39" />

            <TextView
                android:text="TELVE KAZAN"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/telvekazanText"
                app:layout_constraintLeft_toLeftOf="@+id/telvekazanButon"
                app:layout_constraintRight_toRightOf="@+id/telvekazanButon"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/telvekazanButon"
                android:textStyle="normal|bold"
                app:layout_constraintBottom_toTopOf="@+id/guideline3"
                android:layout_marginBottom="8dp"
                app:layout_constraintVertical_bias="0.0" />

            <ImageButton
                android:layout_width="56dp"
                android:layout_height="56dp"
                android:background="@drawable/telvebuton"
                android:id="@+id/telveSayisiImage"
                app:layout_constraintBottom_toBottomOf="@+id/falbaktirButon"
                app:layout_constraintTop_toTopOf="@+id/falbaktirButon"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toLeftOf="@+id/guideline"
                app:layout_constraintVertical_bias="0.39" />

            <TextView
                android:text="13 TELVENİZ\nVAR"
                android:textAlignment="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/telveSayisiTexts"
                android:textStyle="normal|bold"
                app:layout_constraintLeft_toLeftOf="@+id/telveSayisiImage"
                app:layout_constraintRight_toRightOf="@+id/telveSayisiImage"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/telveSayisiImage"
                app:layout_constraintBottom_toTopOf="@+id/guideline2"
                android:layout_marginBottom="8dp"
                app:layout_constraintVertical_bias="0.0" />

            <ImageButton
                android:layout_width="56dp"
                android:layout_height="56dp"
                android:background="@drawable/sssbuton"
                android:id="@+id/sssImage"
                app:layout_constraintLeft_toLeftOf="@+id/telveSayisiImage"
                app:layout_constraintRight_toRightOf="@+id/telveSayisiImage"
                app:layout_constraintTop_toTopOf="@+id/guideline2"
                app:layout_constraintBottom_toTopOf="@+id/guideline3"
                android:layout_marginBottom="8dp"
                app:layout_constraintVertical_bias="0.39" />

            <TextView
                android:text="SIK SORULAN\nSORULAR"
                android:textAlignment="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/sssText"
                app:layout_constraintLeft_toLeftOf="@+id/sssImage"
                app:layout_constraintRight_toRightOf="@+id/sssImage"
                app:layout_constraintBottom_toTopOf="@+id/guideline3"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/sssImage"
                app:layout_constraintVertical_bias="0.0"
                android:textStyle="normal|bold" />

            <ImageButton
                android:layout_width="56dp"
                android:layout_height="56dp"
                android:background="@drawable/kullanimbuton"
                android:id="@+id/kullanimButon"
                app:layout_constraintLeft_toLeftOf="@+id/telveSayisiImage"
                app:layout_constraintRight_toRightOf="@+id/telveSayisiImage"
                app:layout_constraintTop_toTopOf="@+id/guideline3"
                app:layout_constraintBottom_toTopOf="@+id/guideline8"
                android:layout_marginBottom="8dp"
                app:layout_constraintVertical_bias="0.39" />

            <TextView
                android:text="KULLANIM"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/kullanimText"
                android:textStyle="normal|bold"
                app:layout_constraintLeft_toLeftOf="@+id/kullanimButon"
                app:layout_constraintRight_toRightOf="@+id/kullanimButon"
                app:layout_constraintBottom_toTopOf="@+id/guideline8"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/kullanimButon"
                app:layout_constraintVertical_bias="0.0"
                app:layout_constraintHorizontal_bias="1.0" />

            <android.support.constraint.Guideline
                android:layout_width="384dp"
                android:layout_height="wrap_content"
                android:id="@+id/guideline9"
                app:layout_constraintGuide_begin="449dp"
                android:orientation="horizontal"
                tools:layout_editor_absoluteY="449dp"
                tools:layout_editor_absoluteX="0dp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/yataycizgi"
                android:id="@+id/imageView7"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/guideline2" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/yataycizgi"
                android:id="@+id/imageView8"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="@+id/guideline3"
                app:layout_constraintRight_toRightOf="parent" />

            <ImageView
                android:layout_width="wrap_content"
                android:background="@drawable/dikcizgi"
                android:id="@+id/imageView"
                app:layout_constraintRight_toLeftOf="@+id/guideline"
                app:layout_constraintTop_toTopOf="@+id/guideline7"
                app:layout_constraintBottom_toTopOf="@+id/guideline8"
                android:layout_height="wrap_content" />

        </android.support.constraint.ConstraintLayout>

        <include layout="@layout/activity_toolbar">
        </include>

        <android.support.v7.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/toolbarAnasayfa"
            android:minHeight="?attr/actionBarSize"
            android:layout_alignParentTop="true"
            android:background="@color/colorPrimaryDark"
            app:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="FALBOOK"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textSize="18sp"
                android:id="@+id/anasayfa_toolbar_baslik" />

        </android.support.v7.widget.Toolbar>

    </RelativeLayout>


    <android.support.design.widget.NavigationView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/anaNavView"
        app:menu="@menu/menu_nav_bar"
        android:background="@color/colorPrimary"
        app:headerLayout="@layout/nav_header"
        android:layout_gravity = "start"
        >

    </android.support.design.widget.NavigationView>


</android.support.v4.widget.DrawerLayout>